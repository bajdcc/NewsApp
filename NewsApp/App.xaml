<Application x:Class="NewsApp.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:NewsApp"
             xmlns:sys="clr-namespace:System;assembly=mscorlib"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <sys:String x:Key="WindowCaption">NewsApp#bajdcc.782195</sys:String>
        <sys:Double x:Key="MainOverlayHeight">200</sys:Double>
        <sys:Double x:Key="MainOverlayDisplayHeight">50</sys:Double>
        <sys:Double x:Key="MainOverlayTimeWidth">100</sys:Double>
        <sys:Double x:Key="PixelPerSeconds">60</sys:Double>
        <sys:Double x:Key="MarqueeBlank">50</sys:Double>
        <ResourceDictionary x:Key="RssFeed" >
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="RssFeed.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
